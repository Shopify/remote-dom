{
  "name": "@remote-ui/mini-react",
  "version": "1.4.0",
  "publishConfig": {
    "access": "public",
    "@remote-ui:registry": "https://registry.npmjs.org"
  },
  "license": "MIT",
  "main": "index.js",
  "module": "index.mjs",
  "esnext": "index.esnext",
  "types": "./build/ts/index.d.ts",
  "typesVersions": {
    "*": {
      "compat": [
        "./build/ts/compat.d.ts"
      ],
      "htm": [
        "./build/ts/htm-binding.d.ts"
      ],
      "jsx-runtime": [
        "./build/ts/jsx-runtime.d.ts"
      ],
      "jsx-dev-runtime": [
        "./build/ts/jsx-runtime.d.ts"
      ],
      "testing": [
        "./build/ts/testing/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "esnext": "./index.esnext",
      "import": "./index.mjs",
      "require": "./index.js"
    },
    "./compat": {
      "esnext": "./compat.esnext",
      "import": "./compat.mjs",
      "require": "./compat.js"
    },
    "./htm": {
      "esnext": "./htm.esnext",
      "import": "./htm.mjs",
      "require": "./htm.js"
    },
    "./jsx-runtime": {
      "esnext": "./jsx-runtime.esnext",
      "import": "./jsx-runtime.mjs",
      "require": "./jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "esnext": "./jsx-runtime.esnext",
      "import": "./jsx-runtime.mjs",
      "require": "./jsx-runtime.js"
    },
    "./testing": {
      "esnext": "./testing.esnext",
      "import": "./testing.mjs",
      "require": "./testing.js"
    }
  },
  "sideEffects": false,
  "dependencies": {
    "@remote-ui/core": "^2.2.0",
    "htm": "^3.0.0"
  },
  "devDependencies": {
    "@quilted/react-testing": "^0.3.0"
  },
  "peerDependencies": {
    "@quilted/react-testing": "^0.3.0",
    "@remote-ui/async-subscription": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "@remote-ui/async-subscription": {
      "optional": true
    },
    "@quilted/react-testing": {
      "optional": true
    }
  }
}

    "prerelease": "yarn build",
    "release": "lerna publish --skip-npm && git add -A && git push --follow-tags"
  },
  "devDependencies": {
    "@shopify/eslint-plugin": "^36.0.2",
    "@shopify/sewing-kit": "^0.122.0",
    "lerna": "^2.9.0",

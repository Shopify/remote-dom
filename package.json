{
  "name": "remote-ui",
  "version": "0.0.0",
  "private": true,
  "author": "Shopify Inc.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shopify/remote-ui.git"
  },
  "workspaces": [
    "./packages/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "test": "yarn build && sewing-kit test",
